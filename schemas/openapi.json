{
  "openapi": "3.1.0",
  "info": {
    "title": "Tampa Events API",
    "version": "2026-01-25",
    "description": "Community events aggregation API for Tampa Bay tech meetups and events",
    "contact": {
      "name": "Tampa Devs",
      "url": "https://tampa.dev"
    }
  },
  "servers": [
    {
      "url": "https://events.api.tampa.dev",
      "description": "Production"
    },
    {
      "url": "http://localhost:8787",
      "description": "Development"
    }
  ],
  "components": {
    "schemas": {},
    "parameters": {}
  },
  "paths": {
    "/2026-01-25/events": {
      "get": {
        "summary": "Get all events",
        "description": "Returns a list of all upcoming events, optionally filtered by query parameters",
        "tags": [
          "Events"
        ],
        "parameters": [
          {
            "schema": {
              "type": "string",
              "description": "Comma-separated list of group urlnames to filter (e.g., \"tampadevs,suncoast-js\")",
              "example": "tampadevs,suncoast-js"
            },
            "required": false,
            "name": "groups",
            "in": "query"
          },
          {
            "schema": {
              "type": "string",
              "description": "Exclude online events (set to \"1\" to exclude)",
              "example": "1"
            },
            "required": false,
            "name": "noonline",
            "in": "query"
          },
          {
            "schema": {
              "type": "string",
              "description": "Only show events within the next N hours",
              "example": "24"
            },
            "required": false,
            "name": "within_hours",
            "in": "query"
          },
          {
            "schema": {
              "type": "string",
              "description": "Only show events within the next N days",
              "example": "7"
            },
            "required": false,
            "name": "within_days",
            "in": "query"
          },
          {
            "schema": {
              "type": "string",
              "description": "Exclude groups with no events (set to \"1\" to exclude)",
              "example": "1"
            },
            "required": false,
            "name": "noempty",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "List of events",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string"
                      },
                      "title": {
                        "type": "string"
                      },
                      "description": {
                        "type": "string"
                      },
                      "dateTime": {
                        "type": "string"
                      },
                      "duration": {
                        "type": "string"
                      },
                      "eventUrl": {
                        "type": "string"
                      },
                      "status": {
                        "type": "string",
                        "enum": [
                          "ACTIVE",
                          "CANCELLED",
                          "DRAFT"
                        ]
                      },
                      "eventType": {
                        "type": "string",
                        "enum": [
                          "PHYSICAL",
                          "ONLINE",
                          "HYBRID"
                        ]
                      },
                      "rsvpCount": {
                        "type": "number"
                      },
                      "venues": {
                        "type": "array",
                        "items": {
                          "nullable": true
                        }
                      },
                      "photo": {
                        "nullable": true
                      },
                      "group": {
                        "nullable": true
                      },
                      "address": {
                        "type": "string",
                        "nullable": true
                      },
                      "googleMapsUrl": {
                        "type": "string",
                        "nullable": true
                      },
                      "photoUrl": {
                        "type": "string",
                        "nullable": true
                      },
                      "isOnline": {
                        "type": "boolean"
                      }
                    },
                    "required": [
                      "id",
                      "title",
                      "dateTime",
                      "eventUrl",
                      "status",
                      "rsvpCount",
                      "venues",
                      "address",
                      "googleMapsUrl",
                      "photoUrl",
                      "isOnline"
                    ]
                  }
                }
              }
            }
          },
          "503": {
            "description": "Service unavailable - no event data",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },

    "/2026-01-25/schemas/{name}": {
      "get": {
        "summary": "Get specific JSON schema",
        "description": "Returns the JSON Schema for a specific model type",
        "tags": [
          "Schemas"
        ],
        "parameters": [
          {
            "schema": {
              "type": "string",
              "description": "Schema name (event, group, venue, photo, meetup)",
              "example": "event"
            },
            "required": true,
            "name": "name",
            "in": "path"
          }
        ],
        "responses": {
          "200": {
            "description": "JSON Schema",
            "content": {
              "application/schema+json": {
                "schema": {
                  "nullable": true
                }
              }
            }
          },
          "404": {
            "description": "Schema not found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string"
                    },
                    "available": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    }
                  },
                  "required": [
                    "error",
                    "available"
                  ]
                }
              }
            }
          }
        }
      }
    },

{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://api.tampa.dev/2026-01-25/schemas/venue",
  "title": "Venue Schema",
  "description": "Schema for venue/location data",
  "$ref": "#/definitions/Venue",
  "definitions": {
    "Venue": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "address": {
          "type": "string"
        },
        "city": {
          "type": "string"
        },
        "state": {
          "type": "string"
        },
        "postalCode": {
          "type": "string"
        },
        "lat": {
          "type": "number"
        },
        "lon": {
          "type": "number"
        }
      },
      "required": [
        "name"
      ],
      "additionalProperties": false
    }
  }
}
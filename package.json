{
  "name": "events-api-tampa-dev",
  "version": "0.0.0",
  "type": "module",
  "dependencies": {
    "@aws-sdk/s3-request-presigner": "^3.978.0",
    "@cloudflare/workers-oauth-provider": "^0.2.2",
    "@hono/swagger-ui": "^0.5.3",
    "@hono/zod-openapi": "^0.19.10",
    "aws4fetch": "^1.0.20",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.11.7",
    "ics": "^3.8.1",
    "jose": "^6.1.3",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "tslib": "^2.8.1",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.980.0",
    "@cloudflare/workers-types": "^4.20250924.0",
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.10",
    "chai": "^6.2.2",
    "drizzle-kit": "^0.31.8",
    "miniflare": "^4.20260128.0",
    "mocha": "^11.7.2",
    "tsx": "^4.21.0",
    "typescript": "^5.8.3",
    "vitest": "^4.0.18",
    "wrangler": "^4.25.1"
  },
  "private": true,
  "scripts": {
    "start": "wrangler dev",
    "start:https": "wrangler dev --local-protocol https --https-key-path ./certs/tampa.dev+5-key.pem --https-cert-path ./certs/tampa.dev+5.pem --port 8787",
    "deploy": "wrangler deploy --env production",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:integration:watch": "vitest --config vitest.integration.config.ts",
    "generate:encryption-key": "tsx scripts/generate-encryption-key.ts",
    "generate:oidc-keys": "npx tsx scripts/generate-oidc-keys.ts",
    "generate-schemas": "tsx scripts/generate-schemas.ts",
    "generate-openapi": "tsx scripts/generate-openapi.ts",
    "aggregate": "tsx scripts/run-aggregation.ts",
    "test:providers": "tsx scripts/test-providers.ts",
    "db:generate": "drizzle-kit generate --config=drizzle.config.local.ts",
    "db:migrate:local": "wrangler d1 migrations apply events-db --local",
    "db:migrate:remote": "wrangler d1 migrations apply events-db --remote",
    "db:seed:local": "tsx scripts/seed-groups.ts",
    "db:seed:remote": "tsx scripts/seed-groups.ts --remote",
    "db:setup:local": "npm run db:migrate:local && npm run db:seed:local",
    "db:setup:remote": "npm run db:migrate:remote && npm run db:seed:remote",
    "db:studio": "drizzle-kit studio --config=drizzle.config.ts",
    "deploy:staging": "wrangler deploy --env staging",
    "db:migrate:staging": "wrangler d1 migrations apply events-db-staging --remote --env staging",
    "db:seed:staging": "tsx scripts/seed-groups.ts --remote --env staging",
    "db:setup:staging": "npm run db:migrate:staging && npm run db:seed:staging"
  }
}
